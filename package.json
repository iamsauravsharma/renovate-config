{
    "name": "@iamsauravsharma/renovate-config",
    "version": "0.1.3",
    "description": "Renovate config for iamsauravsharma github profile",
    "keywords": [
        "renovate",
        "config",
        "renovate-config",
        "renovate-preset"
    ],
    "homepage": "https://github.com/iamsauravsharma/renovate-config",
    "bugs": {
        "url": "https://github.com/iamsauravsharma/renovate-config/issues"
    },
    "license": "MIT",
    "author": "Saurav Sharma <apppdroiddeveloper@gmail.com> (https://github.com/iamsauravsharma)",
    "repository": {
        "type": "git",
        "url": "https://github.com/iamsauravsharma/renovate-config.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "renovate-config": {
        "default": {
            "extends": [
                ":enableRenovate",
                ":separateMajorReleases",
                ":combinePatchMinorReleases",
                ":renovatePrefix",
                ":semanticCommits",
                ":semanticPrefixChore",
                ":rebaseStalePrs",
                ":prImmediately",
                ":disableRateLimiting",
                ":automergeDisabled",
                ":ignoreUnstable",
                ":updateNotScheduled",
                ":autodetectPinVersions",
                ":gitSignOff",
                ":label(dependencies)",
                "group:allNonMajor",
                "group:monorepos",
                "group:recommended"
            ]
        }
    }
}